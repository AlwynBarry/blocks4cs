{
	"name": "blocks4cs",
	"version": "1.0.0",
	"description": "Gutenberg Blocks to display Data on Events and SmallGroups from ChurchSuite",
	"author": "dramb <ab_email@post.com>",
	"license": "GPL-2.0-or-later",
	"homepage": "https://github.com/AlwynBarry/blocks4cs",
	"main": "build/index.js",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=blocks/src/ --output-path=blocks/build/ --webpack-copy-php --blocks-manifest",
		"build": "wp-scripts build --webpack-src-dir=blocks/src/ --output-path=blocks/build/ --webpack-copy-php --blocks-manifest",
		"start:cs-event-cards": "wp-scripts start --webpack-src-dir=blocks/src/cs-event-cards --output-path=blocks/build/cs-event-cards",
		"build:cs-event-cards": "wp-scripts build --webpack-src-dir=blocks/src/cs-event-cards --output-path=blocks/build/cs-event-cards",
		"start:cs-event-list": "wp-scripts start --webpack-src-dir=blocks/src/cs-event-list --output-path=blocks/build/cs-event-list",
		"build:cs-event-list": "wp-scripts build --webpack-src-dir=blocks/src/cs-event-list --output-path=blocks/build/cs-event-list",
		"start:cs-smallgroups": "wp-scripts start --webpack-src-dir=blocks/src/cs-smallgroups --output-path=blocks/build/cs-smallgroups",
		"build:cs-smallgroups": "wp-scripts build --webpack-src-dir=blocks/src/cs-smallgroups --output-path=blocks/build/cs-smallgroups",
		"start:cs-calendar": "wp-scripts start --webpack-src-dir=blocks/src/cs-calendar --output-path=blocks/build/cs-calendar",
		"build:cs-calendar": "wp-scripts build --webpack-src-dir=blocks/src/cs-calendar --output-path=blocks/build/cs-calendar",
		"build:makepot": "npx makepot"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.14.1"
	},
	"dependencies": {
		"@wordpress/server-side-render": "^5.21.0",
		"@wp-blocks/make-pot": "^1.6.3"
	}
}
